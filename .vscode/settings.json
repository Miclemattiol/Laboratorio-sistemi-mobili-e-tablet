{
    "dart.lineLength": 100000,
    "dart.showTodos": false,
    "cSpell.words": [
        "APPL",
        "autovalidate",
        "fullscreen",
        "housewallet",
        "lerp",
        "lpsmt",
        "mipmap",
        "rxdart",
        "SHORTWEEKDAYS",
        "unfocus"
    ],
    "cSpell.ignorePaths": [
        "package-lock.json",
        "node_modules",
        "vscode-extension",
        ".git/objects",
        ".vscode",
        ".vscode-insiders",
        "lib/data/icons.dart",
    ],
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        ".vscode": true,
        ".dart_tool": true,
        ".idea": true,
        "build": true,
        "house_wallet.iml": true,
        ".flutter-plugins": true,
        ".flutter-plugins-dependencies": true,
        "lib/generated_plugin_registrant.dart": true,
        ".metadata": true,
        "pubspec.lock": true,
        "lib/firebase_options.dart": true
    }
}